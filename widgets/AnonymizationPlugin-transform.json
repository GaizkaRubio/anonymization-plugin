{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Encrypt Plugin",
  "configuration-groups": [
    {
      "label": "Encrypt Configuration",
      "properties": [
        {
          "label": "Mode",
          "name": "anonymizationMode",
          "widget-type": "select",
          "widget-attributes": {
            "default": "Encrypt",
            "values": ["Encrypt", "Decrypt"]
          }
        },
        {
          "label": "Encrypt Function",
          "name": "encryptFunction",
          "widget-type": "select",
          "widget-attributes": {
            "default": "Aead",
            "values": ["Aead", "Deterministic Aead"]
          }
        },
        {
          "label": "Field-Category",
          "name": "categoryConfig",
          "widget-type": "keyvalue",
          "widget-attributes": {
            "delimiter": ",",
            "kv-delimiter": ":",
            "key-placeholder": "Enter key",
            "value-placeholder": "Enter value"
          }
        }
      ,
        {
          "label": "Category-Key",
          "name": "categoryKeyConfig",
          "widget-type": "keyvalue",
          "widget-attributes": {
            "delimiter": ",",
            "kv-delimiter": ":",
            "key-placeholder": "Enter key",
            "value-placeholder": "Enter value"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "label": "schema",
      "widget-type": "schema"
    }
  ]
}
